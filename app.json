{
  "expo": {
    "name": "egfbt-core",
    "slug": "egfbt-core",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./App/App_Assets/images/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./App/App_Assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
       "bundleIdentifier": "com.egfbt.core",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.egfbt.core",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./App/App_Assets/images/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./App/App_Assets/images/images/favicon.png"
    }
  }
}
